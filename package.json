{
  "name": "adonisjs-maintenance",
  "version": "1.0.0",
  "description": "A maintenance mode for the AdonisJS framework",
  "main": "build/providers/HoneypotProvider.js",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node -r @adonisjs/require-ts/build/register bin/test.ts",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelBelgium/adonisjs-maintenance.git"
  },
  "keywords": [
    "adonisjs"
  ],
  "author": "michaelbelgium.me,Michael V",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MichaelBelgium/adonisjs-maintenance/issues"
  },
  "homepage": "https://github.com/MichaelBelgium/adonisjs-maintenance#readme",
  "devDependencies": {
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.13",
    "@types/node": "^20.10.1",
    "del-cli": "^5.1.0",
    "mrm": "^4.1.22",
    "typescript": "^5.3.2"
  },
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [
      "github-actions"
    ],
    "minNodeVersion": "latest",
    "probotApps": [
      "stale"
    ],
    "runGhActionsOnWindows": false
  },
  "files": [
    "build/src",
    "build/index.d.ts",
    "build/index.js"
  ]
}
